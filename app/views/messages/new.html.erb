<h1>Say what you wanna say, on this special day</h1>
<div class="form">
<%= form_for @message do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :title %>
<%= f.text_field :title %>
<br>
<%= f.label :content %>
<%= f.text_area :content %>
<br>
<%= f.submit "Send" %>
<% end %>
</div >
